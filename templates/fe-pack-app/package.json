    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.0",
    "rollup": "^4.24.2",
