{
  "name": "@qlover/fe-utils",
  "description": "A utils for frontwork",
  "version": "1.0.6",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./server": {
      "require": "./dist/server/index.cjs",
      "import": "./dist/server/index.esm.js",
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qlover/fe-base.git",
    "directory": "packages/fe-utils"
  },
  "homepage": "https://github.com/qlover/fe-base/tree/master/packages/fe-utils#readme",
  "keywords": [
    "utils",
    "fe-utils"
  ],
  "author": "qlover",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "build:docs": "typedoc --options ./typedoc.json"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.15.0",
    "@qlover/fe-scripts": "^0.3.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.12",
    "circular-json": "^0.5.9",
    "fs-extra": "^11.2.0",
    "rollup": "^4.24.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typedoc": "~0.25.0",
    "typedoc-plugin-markdown": "~3.17.0",
    "typescript": "^5.6.3"
  }
}
