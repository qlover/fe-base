{
  "name": "@qlover/create-app",
  "version": "0.1.3",
  "type": "module",
  "private": false,
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/es/index.d.ts"
    },
    "./cjs/*": "./dist/cjs/*",
    "./es/*": "./dist/es/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "bin",
    "templates",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "bin": {
    "create-app": "./bin/create-app.js"
  },
  "scripts": {
    "build": "rollup -c",
    "create:app:build": "npm run build && npm run create:app",
    "create:app": "node ./bin/create-app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qlover/fe-base.git",
    "directory": "packages/create-app"
  },
  "homepage": "https://github.com/qlover/fe-base#readme",
  "keywords": [
    "create-app",
    "fe-scripts",
    "scripts"
  ],
  "author": "qlover",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@qlover/scripts-context": "latest"
  },
  "dependencies": {
    "@qlover/fe-utils": "latest",
    "ignore": "^7.0.3",
    "inquirer": "^12.3.2",
    "ora": "^8.1.1"
  }
}
