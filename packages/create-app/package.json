{
  "name": "@qlover/create-app",
  "version": "0.1.20",
  "type": "module",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "configs",
    "templates",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "bin": {
    "create-app": "bin/create-app.js"
  },
  "scripts": {
    "build": "rollup -c",
    "create:app:build": "npm run build && npm run create:app",
    "create:app": "node ./bin/create-app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qlover/fe-base.git",
    "directory": "packages/create-app"
  },
  "homepage": "https://github.com/qlover/fe-base#readme",
  "keywords": [
    "create-app",
    "fe-scripts",
    "scripts"
  ],
  "author": "qlover",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@qlover/env-loader": "^0.0.7"
  },
  "dependencies": {
    "@qlover/fe-corekit": "^1.2.8",
    "@qlover/scripts-context": "^0.0.14",
    "commander": "^13.1.0",
    "ignore": "^7.0.3",
    "inquirer": "^12.3.2",
    "ora": "^8.1.1",
    "lodash": "^4.17.21"
  }
}
