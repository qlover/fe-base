{
  "base": {
    "routes": [
      {
        "path": "/",
        "element": "base/RedirectPathname",
        "meta": {
          "category": "main"
        }
      },
      {
        "path": "/:lng",
        "element": "base/Layout",
        "meta": {
          "category": "main"
        },
        "children": [
          {
            "index": true,
            "element": "base/Home",
            "meta": {
              "title": "fe-utils/home",
              "icon": "home",
              "localNamespace": "home"
            }
          },
          {
            "path": "about",
            "element": "base/About",
            "meta": {
              "title": "关于",
              "icon": "info",
              "localNamespace": "about"
            }
          },
          {
            "path": "jsonStorage",
            "element": "base/JSONStorage",
            "meta": {
              "title": "FE-Utils JSONStorage",
              "icon": "info",
              "localNamespace": "jsonStorage"
            }
          },
          {
            "path": "request",
            "element": "base/Request",
            "meta": {
              "title": "FE-Utils Request",
              "icon": "info",
              "localNamespace": "request"
            }
          },
          {
            "path": "executor",
            "element": "base/Executor",
            "meta": {
              "title": "FE-Utils Executor",
              "icon": "info",
              "localNamespace": "executor"
            }
          },
          {
            "path": "errorIdentifier",
            "element": "base/ErrorIdentifier",
            "meta": {
              "title": "FE-Utils ErrorIdentifier",
              "icon": "info",
              "localNamespace": "common"
            }
          },
          {
            "path": "404",
            "element": "404",
            "meta": {
              "category": "common",
              "title": "404",
              "layout": "blank",
              "localNamespace": "common"
            }
          },
          {
            "path": "500",
            "element": "500",
            "meta": {
              "category": "common",
              "title": "500",
              "layout": "blank",
              "localNamespace": "common"
            }
          }
        ]
      },

      {
        "path": "/:lng",
        "element": "auth/Layout",
        "meta": {
          "category": "auth"
        },
        "children": [
          {
            "index": true,
            "element": "auth/Login"
          },
          {
            "path": "login",
            "element": "auth/Login",
            "meta": {
              "title": "登录",
              "icon": "info",
              "localNamespace": "login"
            }
          },
          {
            "path": "register",
            "element": "auth/Register",
            "meta": {
              "title": "注册",
              "icon": "info",
              "localNamespace": "register"
            }
          }
        ]
      },

      {
        "path": "404",
        "element": "404",
        "meta": {
          "category": "common",
          "title": "404",
          "layout": "blank",
          "localNamespace": "common"
        }
      },
      {
        "path": "500",
        "element": "500",
        "meta": {
          "category": "common",
          "title": "500",
          "layout": "blank",
          "localNamespace": "common"
        }
      },
      {
        "path": "*",
        "element": "404",
        "meta": {
          "category": "common",
          "title": "404",
          "layout": "blank",
          "localNamespace": "common"
        }
      }
    ]
  }
}
