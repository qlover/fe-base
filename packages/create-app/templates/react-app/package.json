{
  "name": "react-app",
  "description": "A react app template",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/qlover/fe-base/tree/master/packages/create-app/templates/react-app#readme",
  "author": "qlover",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qlover/fe-base.git",
    "directory": ""
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "README.en.md",
    "docs"
  ],
  "keywords": [
    "react-app",
    "react-app-template"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --mode localhost",
    "dev:force": "vite --mode localhost --force",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "build:staging": "npm run lint && vite build --mode staging",
    "build:prod": "npm run lint && vite build --mode production",
    "build:analyze": "vite build --mode production && start dist/stats.html",
    "lint": "eslint ./src ./__tests__ ./config ./makes ./e2e",
    "lint:fix": "eslint ./src ./__tests__ ./config ./makes ./e2e --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "fix": "npm run lint:fix && npm run format",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "preview": "vite preview",
    "playwright:postinstall": "npm init playwright@latest --yes \"--\" . '--quiet' '--browser=chromium' '--browser=firefox' '--browser=webkit' '--gha'",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:report": "playwright show-report playwright-report"
  },
  "dependencies": {
    "@brain-toolkit/antd-blocks": "^0.0.1",
    "@brain-toolkit/antd-theme-override": "^0.0.3",
    "@brain-toolkit/react-kit": "^0.1.0",
    "@qlover/corekit-bridge": "latest",
    "@qlover/fe-corekit": "latest",
    "@qlover/logger": "^0.1.1",
    "@qlover/slice-store-react": "^1.4.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "antd": "^5.25.3",
    "clsx": "^2.1.1",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "inversify": "^7.1.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.2.0",
    "react-router-dom": "^7.1.5",
    "reflect-metadata": "^0.2.2",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@brain-toolkit/ts2locales": "^0.2.3",
    "@playwright/test": "^1.56.1",
    "@qlover/env-loader": "latest",
    "@qlover/eslint-plugin": "file:../../eslint-plugin",
    "@qlover/fe-scripts": "latest",
    "@qlover/fe-standard": "latest",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/typo-js": "^1.2.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.12.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.79.4",
    "tailwindcss": "^4.1.8",
    "terser": "^5.41.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-imp": "^2.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
