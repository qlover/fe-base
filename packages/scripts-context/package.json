    "script",
    "scripts-context"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.15",
    "@qlover/fe-standard": "latest"
  },
  "dependencies": {
    "@qlover/fe-utils": "latest",
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "lodash": "^4.17.21",
    "shelljs": "^0.8.5",
    "merge": "^2.1.1"
  }
}
