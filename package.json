{
  "name": "fe-base-monorepo",
  "version": "0.0.0",
  "description": "A base frontwork",
  "type": "module",
  "private": true,
  "repository": {
    "type": "github",
    "url": "https://github.com/qlover/fe-base"
  },
  "homepage": "https://github.com/qlover/fe-base#readme",
  "files": [],
  "scripts": {
    "build": "pnpm -r --workspace-concurrency=4 run build && pnpm rebuild",
    "build:fe-release": "pnpm --filter @qlover/fe-release build",
    "build:logger": "pnpm --filter @qlover/logger build",
    "dryrun:releasePR": "node ./packages/fe-release/dist/cli.js -V -P --githubPR.dry-run-create-PR",
    "dryrun:release": "node ./packages/fe-release/dist/cli.js -V --changelog.skip-changeset --githubPR.dry-run-create-PR",
    "release": "pnpm --filter @qlover/fe-release release",
    "nx:build": "nx affected:build",
    "nx:build:force": "pnpm nx run-many --target=build --all --skip-nx-cache --parallel --maxParallel=4",
    "prettier": "prettier --ignore-path .prettierignore **/*.{js,ts,json,cjs,mjs,.md,.mdx} --write",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "clean": "fe-clean",
    "clean:build": "fe-clean -f packages/*/dist -r",
    "commit": "fe-commit",
    "clean-branch": "fe-clean-branch",
    "check-packages": "fe-check-packages",
    "prepare": "husky"
  },
  "keywords": [
    "fe",
    "fe-base"
  ],
  "author": "qlover",
  "license": "ISC",
  "devEngines": {
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.2",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.13.0",
    "@microsoft/api-extractor": "^7.52.8",
    "@nx/esbuild": "20.6.4",
    "@nx/js": "20.6.4",
    "@qlover/env-loader": "workspace:*",
    "@qlover/eslint-plugin-fe-dev": "workspace:*",
    "@qlover/fe-code2markdown": "workspace:*",
    "@qlover/fe-scripts": "workspace:*",
    "@qlover/fe-standard": "workspace:*",
    "@qlover/fe-release": "workspace:*",
    "@qlover/scripts-context": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.9.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.13.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-vitest": "^0.5.4",
    "glob": "^11.0.3",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "nx": "20.6.4",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.24.2",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
