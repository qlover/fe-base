{
  "name": "@qlover/fe-base",
  "version": "1.0.114",
  "description": "A base frontwork",
  "type": "module",
  "private": true,
  "files": [
    "dist",
    "package.json",
    "REMEAD.md"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/qlover/fe-base"
  },
  "homepage": "https://github.com/qlover/fe-base#readme",
  "scripts": {
    "build": "yarn build:fe-utils",
    "build:fe-utils": "yarn workspace @qlover/fe-utils build",
    "release-pr:fe-utils": "fe-release-pr -p ./packages/fe-utils -V",
    "release:fe-utils": "fe-release -p ./packages/fe-utils -V",
    "release-pr:fe-scripts": "fe-release-pr -p ./packages/fe-scripts -V",
    "release:fe-scripts": "fe-release -p ./packages/fe-scripts -V",
    "dryrun:release-pr:fe-utils": "fe-release-pr -p ./packages/fe-utils -V --dry-run",
    "dryrun:release:fe-utils": "fe-release -p ./packages/fe-utils -V --dry-run",
    "dryrun:release-pr:fe-scripts": "fe-release-pr -p ./packages/fe-scripts -V --dry-run",
    "dryrun:release:fe-scripts": "fe-release -p ./packages/fe-scripts -V --dry-run",
    "prettier": "prettier packages --write",
    "eslint": "eslint . --fix",
    "lint": "eslint . --fix",
    "test": "jest",
    "clean": "fe-clean",
    "commit": "fe-commit",
    "clean-branch": "fe-clean-branch",
    "prepare": "husky",
    "build:fe-utils:docs": "yarn workspace @qlover/fe-utils build:docs",
    "build:fe-code2md:docs": "yarn workspace @qlover/fe-code2markdown build:docs",
    "build:docs": "yarn build:fe-utils:docs && yarn build:fe-code2md:docs"
  },
  "keywords": [
    "fe",
    "fe-base"
  ],
  "author": "qlover",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "devEngines": {
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
