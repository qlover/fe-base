{
  "name": "@qlover/fe-base",
  "version": "1.0.114",
  "description": "A base frontwork",
  "type": "module",
  "private": true,
  "repository": {
    "type": "github",
    "url": "https://github.com/qlover/fe-base"
  },
  "homepage": "https://github.com/qlover/fe-base#readme",
  "scripts": {
    "build": "yarn build:scripts-context && yarn build:fe-utils && yarn build:fe-scripts && yarn build:fe-code2markdown && yarn build:create-app",
    "build:fe-utils": "yarn workspace @qlover/fe-utils build",
    "build:fe-scripts": "yarn workspace @qlover/fe-scripts build",
    "build:fe-code2markdown": "yarn workspace @qlover/fe-code2markdown build",
    "build:create-app": "yarn workspace @qlover/create-app build",
    "build:scripts-context": "yarn workspace @qlover/scripts-context build",
    "release-pr:fe-utils": "fe-release-pr -p ./packages/fe-utils -V",
    "release:fe-utils": "fe-release -p ./packages/fe-utils -V",
    "release-pr:fe-scripts": "fe-release-pr -p ./packages/fe-scripts -V",
    "release:fe-scripts": "fe-release -p ./packages/fe-scripts -V",
    "release:fe-standard": "fe-release -p ./packages/fe-standard -V",
    "release-pr:create-app": "fe-release-pr -p ./packages/create-app -V",
    "release:create-app": "fe-release -p ./packages/create-app -V",
    "dryrun:release-pr:fe-utils": "fe-release-pr -p ./packages/fe-utils -V --dry-run",
    "dryrun:release:fe-utils": "fe-release -p ./packages/fe-utils -V --dry-run",
    "dryrun:release-pr:fe-scripts": "fe-release-pr -p ./packages/fe-scripts -V --dry-run",
    "dryrun:release:fe-scripts": "fe-release -p ./packages/fe-scripts -V --dry-run",
    "dryrun:release-pr:create-app": "fe-release-pr -p ./packages/create-app -V --dry-run",
    "prettier": "prettier --ignore-path .prettierignore **/*.{js,ts,json,cjs,mjs} --write",
    "lint": "eslint . --fix",
    "test": "jest",
    "clean": "fe-clean",
    "clean:build": "fe-clean -f packages/*/dist -r",
    "commit": "fe-commit",
    "clean-branch": "fe-clean-branch",
    "check-packages": "fe-check-packages",
    "prepare": "husky",
    "build:fe-utils:docs": "yarn workspace @qlover/fe-utils build:docs",
    "build:fe-code2md:docs": "yarn workspace @qlover/fe-code2markdown build:docs",
    "build:docs": "yarn build:fe-utils:docs && yarn build:fe-code2md:docs"
  },
  "keywords": [
    "fe",
    "fe-base"
  ],
  "author": "qlover",
  "license": "ISC",
  "devEngines": {
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.0",
    "rollup": "^4.24.2",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
